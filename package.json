{
  "name": "metaledger",
  "version": "0.1.0",
  "description": "An opinionated, graph-based frontend to Ledger with added personal finance tools",
  "main": "index.js",
  "scripts": {
    "build": "lsc -c server.ls && lsc -co www www && pug -o www www",
    "watch": "lsc -cw server.ls & lsc -cwo www www & pug -wo www www",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yousefamar/metaledger.git"
  },
  "keywords": [
    "ledger",
    "personal",
    "finance",
    "frontend",
    "front",
    "end",
    "UI",
    "GUI",
    "budget",
    "budgeting",
    "accounting",
    "money",
    "management"
  ],
  "author": "Yousef Amar <yousef@amar.io> (http://yousefamar.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yousefamar/metaledger/issues"
  },
  "homepage": "https://github.com/yousefamar/metaledger#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "ledger-cli": "^0.2.0"
  },
  "devDependencies": {
    "livescript": "^1.5.0",
    "pug-cli": "^1.0.0-alpha6"
  }
}
